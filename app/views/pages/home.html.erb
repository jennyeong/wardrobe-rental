<div class="mx-0 px-0 text-center mb-5">
  <h1 class="w-100"><b>Welcome to Wardrobe</b></h1>
  <h5>Your one stop rental platform</h3>
  <br>
  <%= form_tag "/outfits", method: "GET" do %>
    <%= label_tag :search_outfits %>
    <%= text_field_tag :search_outfits, params[:search_outfits] %>
    <%= submit_tag "Search" %>
  <% end %>
</div>
<div class="drops text-center">
  <h3><b>Drops</b></h3>
  <p>Place cloths cards here</p>
</div>
